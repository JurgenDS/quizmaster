{
    "name": "quizmaster-fe",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "packageManager": "pnpm@10.0.0",
    "scripts": {
        "playwright:install": "playwright install --with-deps",
        "dev": "vite",
        "build": "tsc -b && vite build",
        "check": "tsc -b && biome ci",
        "format": "biome format --write",
        "lint": "biome lint",
        "lint:fix": "biome lint --fix",
        "style": "pnpm run lint:fix && pnpm run format",
        "preview": "vite preview",
        "test:e2e": "bddgen && wait-on tcp:8080 && playwright test",
        "test:e2e:ui": "bddgen && wait-on tcp:5173 && cross-env FE_PORT=5173 playwright test --ui-host=localhost --ui-port=3333"
    },
    "dependencies": {
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-router-dom": "7.1.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@cucumber/cucumber": "11.1.0",
        "@playwright/test": "1.49.0",
        "@types/node": "22.10.1",
        "@types/react": "19.0.8",
        "@types/react-dom": "19.0.3",
        "@vitejs/plugin-react": "4.3.4",
        "cross-env": "7.0.3",
        "esbuild-register": "3.6.0",
        "playwright-bdd": "8.2.0",
        "sass": "1.82.0",
        "typescript": "5.7.2",
        "vite": "6.0.2",
        "vite-tsconfig-paths": "5.1.3",
        "wait-on": "8.0.1"
    }
}
